{
  "name": "fortran",
  "rules": {
    "translation_unit": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_top_level_item"
      }
    },
    "_top_level_item": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "program_statement"
        }
      ]
    },
    "program_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "PREC_RIGHT",
          "value": 0,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "PATTERN",
                "value": "program|PROGRAM"
              },
              {
                "type": "SYMBOL",
                "name": "identifier"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\n"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_top_level_item"
          }
        },
        {
          "type": "PREC_RIGHT",
          "value": 0,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "end_statement"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "PATTERN",
                        "value": "program|PROGRAM"
                      },
                      {
                        "type": "CHOICE",
                        "members": [
                          {
                            "type": "SYMBOL",
                            "name": "identifier"
                          },
                          {
                            "type": "BLANK"
                          }
                        ]
                      }
                    ]
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              }
            ]
          }
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_]\\w*"
    },
    "end_statement": {
      "type": "PATTERN",
      "value": "end|END"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "!"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": [],
  "PREC": {
    "ASSIGNMENT": -10,
    "DEFAULT": 0,
    "LOGICAL_XOR": 5,
    "LOGICAL_EQUIV": 5,
    "LOGICAL_OR": 10,
    "LOGICAL_AND": 20,
    "LOGICAL_NOT": 30,
    "RELATIONAL": 40,
    "ADDITIVE": 50,
    "MULTIPLICATIVE": 60,
    "EXPONENT": 70
  }
}